{
    "name": "cinemagraphr",
    "version": "1.0.0",
    "description": "Cinemagraphr",
    "main": "index.js",
    "scripts": {
        "firebase-init": "firebase login && firebase init",
        "start": "webpack-dev-server",
        "build-dev": "webpack --config=webpack.config",
        "build-prod": "webpack --config=webpack.prod.config",
        "build": "webpack --config=webpack.prod.config",
        "analyze": "BUILD_MODE=analyze yarn build",
        "deploy-dev": "firebase use dev && yarn build-dev && firebase deploy",
        "deploy-prod": "firebase use prod && yarn build-prod && firebase deploy",
        "deploy-all": "yarn deploy-dev && yarn deploy-prod && firebase use dev",
        "deploy-functions-dev": "firebase use dev && firebase deploy --only functions",
        "deploy-functions-prod": "firebase use prod && firebase deploy --only functions",
        "deploy-functions": "yarn deploy-functions-dev && yarn deploy-functions-prod && firebase use dev",
        "debug-function": "firebase experimental:functions:shell"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/itsmoops/cinemagraphr.git"
    },
    "keywords": [
        "react",
        "redux",
        "rebass",
        "firebase",
        "cinemagraph"
    ],
    "engines": {
        "node": "8"
    },
    "author": "Eric Moore",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/itsmoops/cinemagraphr/issues"
    },
    "homepage": "https://github.com/itsmoops/cinemagraphr#readme",
    "dependencies": {
        "autosize": "^4.0.2",
        "capture-video-frame": "^0.1.3",
        "firebase": "^7.8.2",
        "firebase-admin": "^8.9.2",
        "howler": "^2.0.12",
        "lodash.throttle": "^4.1.1",
        "prop-types": "^15.5.10",
        "react": "^16.4.0",
        "react-avatar-editor": "^10.3.0",
        "react-burger-menu": "^2.5.0",
        "react-click-outside": "tj/react-click-outside",
        "react-dom": "^16.4.0",
        "react-dropzone": "^4.2.11",
        "react-ga": "^2.5.3",
        "react-icons-kit": "^1.1.5",
        "react-onclickoutside": "^6.7.1",
        "react-redux": "^5.0.7",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "react-tooltip": "^3.6.1",
        "rebass": "^1.0.7",
        "redux": "^4.0.0",
        "redux-immutable-state-invariant": "^2.1.0",
        "redux-thunk": "^2.3.0",
        "style-loader": "^0.21.0",
        "styled-components": "^3.3.2",
        "styled-components-theme": "^1.0.5",
        "uglifyjs-webpack-plugin": "^1.2.5",
        "uuid": "^3.2.1",
        "webpack": "^4.11.1",
        "webpack-cli": "^3.0.6"
    },
    "devDependencies": {
        "babel-core": "^6.24.1",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^7.0.0",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-syntax-async-functions": "^6.13.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "copy-webpack-plugin": "^4.2.0",
        "eslint": "^4.5.0",
        "eslint-config-airbnb": "^15.1.0",
        "eslint-config-standard": "^10.2.1",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "^5.1.1",
        "eslint-plugin-node": "^5.1.1",
        "eslint-plugin-promise": "^3.5.0",
        "eslint-plugin-react": "^7.3.0",
        "eslint-plugin-standard": "^3.0.1",
        "eslint-plugin-styled-components-config": "0.0.2",
        "file-loader": "^1.1.11",
        "firebase-functions": "^1.1.0",
        "firebase-tools": "^7.13.1",
        "image-webpack-loader": "^3.3.1",
        "webpack-bundle-analyzer": "^2.13.1",
        "webpack-dev-server": "^3.1.4"
    }
}
